    "qrcode": "^1.5.4",
    "qs": "^6.14.0",
    "responsive-storage": "^2.2.0",
    "sortablejs": "^1.15.6",
    "vue": "3.6.0-alpha.6",
    "vue-router": "^4.6.3",
    "vue-tippy": "^6.7.1",
    "vue-types": "^6.0.0"
  },
  "devDependencies": {
    "@alova/adapter-axios": "^2.0.16",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@commitlint/types": "^20.0.0",
    "@faker-js/faker": "^10.1.0",
    "@iconify/json": "^2.2.413",
    "@iconify/vue": "4.2.0",
