    "boxen": "^8.0.1",
    "code-inspector-plugin": "^1.2.10",
    "cssnano": "^7.1.1",
    "gradient-string": "^3.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "oxfmt": "^0.16.0",
    "oxlint": "^1.31.0",
    "playwright": "^1.57.0",
    "postcss": "^8.5.6",
