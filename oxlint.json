{
  "rules": {
    "no-unused-vars": "error",
    "no-undef": "off",
    "no-console": "off",
    "no-debugger": "warn",
    "no-explicit-any": "off",
    "no-empty-interface": "warn",
    "no-parameter-properties": "warn",
    "prefer-as-const": "warn",
    "no-empty-function": "off",
    "no-non-null-assertion": "off",
    "no-unused-expressions": "off",
    "no-unsafe-function-type": "off",
    "no-import-type-side-effects": "error",
    "explicit-module-boundary-types": "off",
    "consistent-type-imports": "off",
    "prefer-literal-enum-member": "error",
    "no-require-imports": "off",
    "no-redeclare": "off",
    "ban-ts-comment": "off",
    "prettier": "warn"
  },
  "vue-rules": {
    "no-v-html": "off",
    "require-default-prop": "off",
    "require-explicit-emits": "off",
    "multi-word-component-names": "off",
    "no-setup-props-reactivity-loss": "off"
  },
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "document": "readonly",
    "console": "readonly",
    "Image": "readonly",
    "RefType": "readonly",
    "EmitType": "readonly",
    "TargetContext": "readonly",
    "ComponentRef": "readonly",
    "ElRef": "readonly",
    "ForDataType": "readonly",
    "AnyFunction": "readonly",
    "PropType": "readonly",
    "Writable": "readonly",
    "Nullable": "readonly",
    "NonNullable": "readonly",
    "Recordable": "readonly",
    "ReadonlyRecordable": "readonly",
    "Indexable": "readonly",
    "DeepPartial": "readonly",
    "Without": "readonly",
    "Exclusive": "readonly",
    "TimeoutHandle": "readonly",
    "IntervalHandle": "readonly",
    "Effect": "readonly",
    "ChangeEvent": "readonly",
    "WheelEvent": "readonly",
    "ImportMetaEnv": "readonly",
    "Fn": "readonly",
    "PromiseFn": "readonly",
    "ComponentElRef": "readonly",
    "parseInt": "readonly",
    "parseFloat": "readonly",
    "$": "readonly",
    "$$": "readonly",
    "$computed": "readonly",
    "$customRef": "readonly",
    "$ref": "readonly",
    "$shallowRef": "readonly",
    "$toRef": "readonly"
  },
  "ignore": ["node_modules/", "dist/", "build/"],
  "prettier": {
    "bracketSpacing": true,
    "singleQuote": true,
    "arrowParens": "avoid",
    "trailingComma": "all",
    "semi": false,
    "printWidth": 160
  },
  "formatWithTabs": false,
  "tabWidth": 2,
  "lineWidth": 160,
  "singleQuote": true,
  "trailingComma": "all",
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "semi": false
}
